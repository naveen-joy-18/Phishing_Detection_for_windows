{
    "manifest_version": 3,
    "name": "Phishing Detective",
    "version": "1.0",
    "description": "Detects phishing and malicious links.",
    "permissions": ["tabs", "activeTab", "webNavigation", "notifications"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    }
  }
  